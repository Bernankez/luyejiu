    "fix": "eslint . --fix && pnpm stylelint:fix",
    "stylelint": "stylelint **/*.{css,vue}",
    "stylelint:fix": "stylelint **/*.{css,vue} --fix"
  },
  "dependencies": {
    "@pinia/nuxt": "^0.4.7",
    "consola": "3.0.0-3",
    "howler": "^2.2.3",
    "pinia": "^2.0.33"
  },
  "devDependencies": {
    "@bernankez/eslint-config": "^0.4.2",
    "@bernankez/stylelint-config": "^0.2.1",
    "@iconify/json": "^2.2.43",
    "@nuxt/devtools": "^0.3.1",
    "@nuxtjs/eslint-module": "^4.0.2",
    "@nuxtjs/i18n": "8.0.0-beta.10",
    "@nuxtjs/i18n-edge": "8.0.0-beta.10-28003311.2c39470",
    "@nuxtjs/stylelint-module": "^5.0.1",
    "@types/howler": "^2.2.7",
    "@unocss/nuxt": "^0.50.6",
    "@vite-pwa/nuxt": "^0.0.7",
    "@vueuse/core": "^9.13.0",
    "@vueuse/nuxt": "^9.13.0",
    "eslint": "^8.37.0",
    "lint-staged": "^13.2.0",
    "lru-cache": "^8.0.5",
    "nuxt": "^3.3.3",
    "nuxt-headlessui": "^1.1.1",
    "nuxt-typed-router": "^3.0.9",
    "nuxt-vitest": "^0.6.9",
    "simple-git-hooks": "^2.8.1",
    "stylelint": "^15.4.0",
    "typescript": "^5.0.3",
    "vue-tsc": "^1.2.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{css,vue}": "stylelint --fix",
    "*": "eslint --fix"
  }
